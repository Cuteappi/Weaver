{
	"name": "weaver",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"dev": "bun convex dev --once && concurrently -r bun:dev:web bun:dev:convex",
		"dev:web": "vite dev",
		"dev:ts": "tsc -b -w",
		"dev:convex": "bun convex dev",
		"build": "vite build && tsc --noEmit",
		"start": "node .output/server/index.mjs",
		"format": "prettier --write ."
	},
	"dependencies": {
